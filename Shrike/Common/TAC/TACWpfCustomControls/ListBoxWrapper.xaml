<UserControl x:Class="TACWpfCustomControls.ListBoxWrapper"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" xmlns:TACWpfCustomControls="clr-namespace:TACWpfCustomControls">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TACWpfCustomControls:ListBoxEx HorizontalAlignment="Left" x:Name="listBoxEx" VerticalAlignment="Top" Grid.Row="0" />
    </Grid>
</UserControl>
